{
  "name": "urban-projects",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "engines": {
    "node": "14.x"
  },
  "scripts": {
    "watch:sass": "node-sass sass/main.scss public/css/style.css -w",
    "devserver": "live-server",
    "start:css": "npm-run-all --parallel watch:sass devserver",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node server.js",

    "compile:sass": "node-sass sass/main.scss css/style.comp.css",
    "prefix:css": "postcss --use autoprefixer -b 'last 10 versions' css/style.comp.css -o css/style.prefix.css",
    "compress:css": "node-sass css/style.prefix.css css/style.css --output-style compressed",
    "build:css": "npm-run-all compile:sass prefix:css compress:css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NickAssassin666/Urban-Projects.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/NickAssassin666/Urban-Projects/issues"
  },
  "homepage": "https://github.com/NickAssassin666/Urban-Projects#readme",
  "dependencies": {
    "body-parser": "^1.20.0",
    "bootstrap": "^5.1.3",
    "dotenv": "^16.0.0",
    "ejs": "^3.1.6",
    "express": "^4.17.3",
    "multer": "^1.4.4",
    "nodemailer": "^6.7.3",
    "nodemon": "^2.0.15"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.4",
    "node-sass": "^7.0.1",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^9.1.0"
  }
}
